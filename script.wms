roll 0 10
move 0 10
set ~ 10 # ~ means dont change
pen 0 # pen up
pen 1 # pen down
pen # toggle pen
get x
get y
get pen
get pos
get size # get canvas size
reset # alias for `set 0 0` and `pen 0`
quit
exit
version
setsize 500 5000
echo We done here
blocking 1
blocking 0
blocking # toggle
sync 0
sync 1
sync # toggle
get sync
get blocking
exec rect.wms # execute file in working directory
setlimits 90 70 # set x and y power limits to 90 and 70
setlimits # reset limits to default